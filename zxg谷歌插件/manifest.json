{
	"manifest_version": 2,
	"name": "15home",
	"version": "1.0.0",
	"description": "__MSG_pluginDesc__",
	"icons":
	{
		"16": "img/LOGO.png",
		"48": "img/LOGO.png",
		"128": "img/LOGO.png"
	},
	"background": {
		"scripts": [ "js/stock_code.js","js/youjian.js","js/getCookie.js"]
	 },
	"browser_action": 
	{
		"default_icon": "img/LOGO.png"
	},
	"content_scripts": 
	[
		{
			"matches": ["<all_urls>"],
			"js": [
				"js/jquery-1.8.3.js", 
				"js/jquery-1.9.1.js",
				"js/jquery-ui-1.31.1.js",
				"js/jquery.cookie.js",
				"js/content-script.js",
                "js_selectTree/demo.js",
                "js_selectTree/jquery-1.11.2.min.js",
                "js_selectTree/jquery.ztree.all.js",
                "js_selectTree/jquery.ztree.exhide.js",
                "js_selectTree/MultipleTreeSelect.js",
                "js_selectTree/MultipleTreeSelect.min.js",
                "js_selectTree/treeSelect.2.0.js"

			],
			"css": ["css/custom.css","css/jquery-ui.css"],
			"run_at": "document_start"
		},
		{
			"matches": ["*://*/*.png", "*://*/*.jpg", "*://*/*.gif", "*://*/*.bmp"],
			"js": ["js/show-image-content-size.js"]
		}
	],
	"permissions":
	[
		"cookies",
		"contextMenus", 
		"bookmarks",
		"tabs", 
		"notifications", 
		"webRequest", 
		"webRequestBlocking", 
		"storage", 
		"http://*/*", 
		"https://*/*",
		"http://apps.bdimg.com/libs/jqueryui/1.10.4/css/jquery-ui.min.css",
		"http://apps.bdimg.com/libs/jquery/1.10.2/jquery.min.js",
		"http://apps.bdimg.com/libs/jqueryui/1.10.4/jquery-ui.min.js"
	],
	"web_accessible_resources": ["js/jquery-1.8.3.js", "js/inject.js","js_selectTree/demo.js","js_selectTree/jquery-1.11.2.min.js","js_selectTree/jquery.ztree.all.js","js_selectTree/jquery.ztree.exhide.js","js_selectTree/MultipleTreeSelect.js","js_selectTree/MultipleTreeSelect.min.js","js_selectTree/treeSelect.2.0.js"],
	"homepage_url": "https://whuzxq.com",
	"chrome_url_overrides":
	{
		"newtab": "tab示例.html"
	},
	 "default_locale": "zh_CN"

}